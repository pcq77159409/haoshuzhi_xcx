.list {
  width: 690rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.list .phonelist-box {
  width: 284rpx;
  height: 167rpx;
  background-color: #FFFFFF;
  position: relative;
  border-radius: 10rpx;
  border: 1px solid #e5e5e5;
  margin-top: 20rpx;
  padding: 0 21rpx 21rpx;
}
.list .phonelist-box image {
  width: 46rpx;
  height: 28rpx;
  position: absolute;
  right: 0;
  top: 0;
}
.list .phonelist-box .number {
  font-size: 30rpx;
  color: #333333;
  padding-top: 26rpx;
}
.list .phonelist-box .move_box {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 20rpx;
  margin-top: 16rpx;
}
.list .phonelist-box .move_box text:first-child {
  color: #666666;
}
.list .phonelist-box .move_box text:last-child {
  color: #fe5858;
}
.list .phonelist-box .include {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 16rpx;
  color: #fe5858;
}
.list .phonelist-box .include text:first-child {
  font-size: 20rpx;
}
.list .phonelist-box .include text:last-child {
  font-size: 28rpx;
}

