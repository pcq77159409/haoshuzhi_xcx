.tabr_box {
  width: 680rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  margin: 60rpx auto 0;
}
.tabr_box .tab {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 25%;
  margin-bottom: 20rpx;
}
.tabr_box .tab image {
  width: 97rpx;
  height: 97rpx;
}
.tabr_box .tab text {
  font-size: 24rpx;
  color: #333333;
}

